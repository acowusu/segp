{
  "name": "desktop",
  "private": true,
  "version": "0.0.3",
  "repository": "https://github.com/acowusu/segp.git",
  "postinstall": "electron-builder install-app-deps",
  "author": "Imperial College London Department of Computing",
  "description": "This project aims to develop an application that seamlessly converts large PDF reports into dynamic and informative videos.",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "rebuild": "electron-rebuild -f -w better-sqlite3",
    "rebuild2": ".\\node_modules\\.bin\\electron-rebuild.cmd -f -m ./node_modules/better-sqlite3",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "build": "tsc && vite build && electron-builder",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
    }
  },
  "overrides": {
    "node-canvas": "npm:dry-uninstall",
    "canvas": "npm:dry-uninstall",
    "unpdf": {
      "canvas": "npm:dry-uninstall"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
